<div class="container mt-5">
            <div class="row">
                <div class="col-lg-12">
                    <!-- Post content-->
                    <article>
                        <!-- Post header-->
                        <header class="mb-5">
                            <!-- Post title-->
                            <h1 class="fw-bolder mb-1">{{Title}}</h1>
                            <!-- Post meta content-->
                            <div class="text-muted fst-italic mb-2">Posted on {{Presented}} by {{Name}} ({{division.data.attributes.Name}})</div>
                        </header>
                        <!-- Preview image figure-->
                        <figure class="mb-4"><center>
                        {{!--     <img class="img-fluid rounded banner" src="https://dummyimage.com/900x400/ced4da/6c757d.jpg" alt="..." />--}}
                            {{!-- <iframe width="600" height="355" src="https://www.youtube.com/embed/o5RfId9Bkos" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe> --}}
                            <video width="600" height="355" src="{{hostAddress}}{{Video.data.attributes.url}}" controls="">
                            
                            </video>
                        </center></figure> 
                        <!-- Post content-->
                        <section class="mb-5">
                            <h2 class="fw-bolder mb-4 mt-5">Attachment(s) :</h2>
                            <ul>
                            {{#each Attachments.data}}
                                <li><a target="_blank" href="{{../hostAddress}}{{attributes.url}}" download>{{attributes.name}}</a></li>
                            {{/each}}
                            </ul>
                        </section>
                        <section class="mb-5">
                            <h2 class="fw-bolder mb-4 mt-5">Participant(s) :</h2>
                            <table class="table">
                                <thead>
                                    <tr>
                                    <th scope="col">No</th>
                                    <th scope="col">Name</th>
                                    <th scope="col">Company</th>
                                    <th scope="col">Division</th>
                                    <th scope="col">Email</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {{#each participants.data}}
                                    <tr>
                                    <th scope="row">{{eval @index " + 1"}}</th>
                                    <td>{{attributes.Name}}</td>
                                    <td>{{attributes.Company}}</td>
                                    <td>{{attributes.Division}}</td>
                                    <td>{{attributes.Email}}</td>
                                    </tr>
                                    {{/each}}
                                </tbody>
                            </table>
                        </section>
                        <section class="mb-5">
                            <h2 class="fw-bolder mb-4 mt-5">Description :</h2>
                            <p class="fs-5 mb-4">{{Description}}</p>
                        </section>
                         <h2 class="fw-bolder mb-4 mt-5">Activity :</h2>
                        <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
                            <div class="carousel-indicators">
                                {{#each Images.data}}
                                <button type="button" data-bs-target="#carouselExampleCaptions" class="{{#ifEquals @index 0}} active{{/ifEquals}}" {{#ifEquals @index 0}} aria-current="true"{{/ifEquals}} data-bs-slide-to="{{@index}}" aria-current="true" aria-label="Slide {{@index}}"></button>
                                {{/each}}
                                {{!-- <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
                                <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button> --}}
                            </div>
                            <div class="carousel-inner">
                                {{#each Images.data}}
                                <div class="carousel-item {{#ifEquals @index 0}} active{{/ifEquals}}">
                                <img src="{{../hostAddress}}{{attributes.url}}" class="gallery d-block w-100" alt="...">
                                </div>
                                {{/each}}
                                {{!-- <div class="carousel-item active">
                                <img src="{{hostAddress}}{{Images.data.[0].attributes.url}}" class="gallery d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item">
                                <img src="{{hostAddress}}{{Images.data.[1].attributes.url}}" class="gallery d-block w-100" alt="...">
                                </div>
                                <div class="carousel-item">
                                <img src="{{hostAddress}}{{Images.data.[2].attributes.url}}" class="gallery d-block w-100" alt="...">
                                </div> --}}
                            </div>
                            <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
                                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                <span class="visually-hidden">Previous</span>
                            </button>
                            <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
                                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                <span class="visually-hidden">Next</span>
                            </button>
                        </div>
                    </article>
                </div>
            </div>
        </div>